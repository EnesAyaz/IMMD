doi = {10.1109/TIA.2011.2161431},
file = {:E$\backslash$:/mendeley/Tallam et al/IEEE Transactions on Industry Applications/Tallam et al. - 2011 - Integrated differential-mode and common-mode filter to mitigate the effects of long motor leads on AC drives.pdf:pdf},
isbn = {9781424452866},
issn = {00939994},
journal = {IEEE Transactions on Industry Applications},
keywords = {AC drives,common mode,differential mode,passive,pulsewidth modulation (PWM),reflected wave},
mendeley-tags = {passive},
number = {5},
pages = {2075--2083},
title = {{Integrated differential-mode and common-mode filter to mitigate the effects of long motor leads on AC drives}},
volume = {47},
year = {2011}
