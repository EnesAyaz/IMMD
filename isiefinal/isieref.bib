file = {:E$\backslash$:/mendeley/Dwari, Parsa, Lipo/PESC Record - IEEE Annual Power Electronics Specialists Conference/Dwari, Parsa, Lipo - 2007 - Optimum control of a five-phase integrated modular permanent magnet motor under normal and open-circuit faul.pdf:pdf},
